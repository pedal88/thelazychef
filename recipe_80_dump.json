[INSTRUCTIONS]
[
  {
    "id": 548,
    "recipe_id": 80,
    "phase": "Prep",
    "component": "Curry Chicken",
    "step_number": 1,
    "text": "Slice the chicken breast into uniform, bite-sized pieces (about 1-inch cubes)."
  },
  {
    "id": 549,
    "recipe_id": 80,
    "phase": "Prep",
    "component": "Curry Chicken",
    "step_number": 2,
    "text": "Season the chicken pieces lightly with salt and pepper."
  },
  {
    "id": 550,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Curry Chicken",
    "step_number": 3,
    "text": "Once the Red Thai Curry sauce is simmering (see Step 5 of the curry component), gently add the seasoned chicken pieces."
  },
  {
    "id": 551,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Curry Chicken",
    "step_number": 4,
    "text": "Ensure the chicken is fully submerged in the sauce and reduce the heat slightly to maintain a gentle simmer."
  },
  {
    "id": 552,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Curry Chicken",
    "step_number": 5,
    "text": "Simmer for 8 to 10 minutes, stirring occasionally, until the chicken is cooked through and no longer pink inside."
  },
  {
    "id": 553,
    "recipe_id": 80,
    "phase": "Serve",
    "component": "Curry Chicken",
    "step_number": 6,
    "text": "Keep the chicken submerged in the finished curry sauce until ready to serve with the rice."
  },
  {
    "id": 554,
    "recipe_id": 80,
    "phase": "Prep",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 1,
    "text": "Slice the onion thinly and cut the bell peppers into strips. Bruise the lemongrass stalk by bending and smashing it a few times."
  },
  {
    "id": 555,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 2,
    "text": "Heat the sunflower oil in a large pot or wok over medium heat. Add the curry sauce (or paste) and chili peppers and stir constantly for 1-2 minutes until fragrant."
  },
  {
    "id": 556,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 3,
    "text": "Add the sliced onion to the pot and saut\u00e9 until translucent, about 3 minutes."
  },
  {
    "id": 557,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 4,
    "text": "Pour in the canned coconut milk and chicken broth, along with the lemongrass, fish sauce, and brown sugar. Bring the mixture to a gentle simmer."
  },
  {
    "id": 558,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 5,
    "text": "Allow the curry base to simmer for 5 minutes to infuse the flavors. (Add the chicken now, referring to the 'Curry Chicken' steps)."
  },
  {
    "id": 559,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 6,
    "text": "Stir in the bell pepper strips and continue to simmer for another 5 minutes, ensuring the peppers remain slightly crisp."
  },
  {
    "id": 560,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 7,
    "text": "Remove the lemongrass stalk. Stir in the lime juice and adjust seasoning (salt/sugar/fish sauce) to taste."
  },
  {
    "id": 561,
    "recipe_id": 80,
    "phase": "Serve",
    "component": "Red Thai Curry Sauce & Vegetables",
    "step_number": 8,
    "text": "Ladle the curry over the steamed rice and garnish generously with fresh basil."
  },
  {
    "id": 562,
    "recipe_id": 80,
    "phase": "Prep",
    "component": "Steamed Jasmine Rice",
    "step_number": 1,
    "text": "Rinse the white rice thoroughly under cold running water in a fine-mesh sieve until the water runs completely clear."
  },
  {
    "id": 563,
    "recipe_id": 80,
    "phase": "Prep",
    "component": "Steamed Jasmine Rice",
    "step_number": 2,
    "text": "Combine the rinsed rice, measured water, and salt in a medium saucepan."
  },
  {
    "id": 564,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Steamed Jasmine Rice",
    "step_number": 3,
    "text": "Bring the mixture to a rapid, rolling boil over high heat, uncovered."
  },
  {
    "id": 565,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Steamed Jasmine Rice",
    "step_number": 4,
    "text": "Immediately reduce the heat to the lowest possible setting, cover the pan tightly with a lid, and simmer without disturbing for exactly 15 minutes."
  },
  {
    "id": 566,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Steamed Jasmine Rice",
    "step_number": 5,
    "text": "After 15 minutes, remove the saucepan from the heat, but keep the lid sealed."
  },
  {
    "id": 567,
    "recipe_id": 80,
    "phase": "Cook",
    "component": "Steamed Jasmine Rice",
    "step_number": 6,
    "text": "Allow the rice to steam (rest) for an additional 5-10 minutes off the heat."
  },
  {
    "id": 568,
    "recipe_id": 80,
    "phase": "Serve",
    "component": "Steamed Jasmine Rice",
    "step_number": 7,
    "text": "Fluff the rice gently with a fork before serving immediately alongside the Red Thai Curry."
